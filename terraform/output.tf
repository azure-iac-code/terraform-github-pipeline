    secondary_access_key             = nonsensitive(azurerm_storage_account.st.secondary_access_key)
    primary_connection_string        = nonsensitive(azurerm_storage_account.st.primary_connection_string)
    secondary_connection_string      = nonsensitive(azurerm_storage_account.st.secondary_connection_string)
    primary_blob_connection_string   = nonsensitive(azurerm_storage_account.st.primary_blob_connection_string)
    secondary_blob_connection_string = nonsensitive(azurerm_storage_account.st.secondary_blob_connection_string)
  }
}